{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
{% for label, messages in app.flashes(['warning', 'error', 'success', 'notice']) %}
    {% for message in messages %}
        <div class="alert alert-{{ label }}">
            {{ message }}
        </div>
    {% endfor %}
{% endfor %}
    <div id="app"></div>
{% endblock %}
